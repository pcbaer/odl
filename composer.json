{
    "name": "hallerbach-it/odl",
    "description": "A Symfony application that collects, analyzes and visualizes ODL data from BfS.",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Sascha Ternes",
            "email": "info@hallerbach-it.de",
            "role": "Developer"
        }
    ],
    "homepage": "https://www.hallerbach-it.de",
    "time": "2021-11-04",
    "version": "1.0.1",

    "require": {
        "php": "~7.4|~8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",

        "components/jquery": "~3.6",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.7",
        "moment/moment": "~2.29",
        "nnnick/chartjs": "~2.9",
        "phpdocumentor/reflection-docblock": "^5.3",
        "sensio/framework-extra-bundle": "~5.6",
        "symfony/apache-pack": "~1.0",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "~1.17",
        "symfony/form": "5.1.*",
        "symfony/framework-bundle": "5.1.*",
        "symfony/http-client": "5.1.*",
        "symfony/intl": "5.1.*",
        "symfony/mailer": "5.1.*",
        "symfony/monolog-bundle": "~3.7",
        "symfony/notifier": "5.1.*",
        "symfony/process": "5.1.*",
        "symfony/property-access": "5.1.*",
        "symfony/property-info": "5.1.*",
        "symfony/proxy-manager-bridge": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/serializer": "5.1.*",
        "symfony/string": "5.1.*",
        "symfony/translation": "5.1.*",
        "symfony/twig-bundle": "5.1.*",
        "symfony/validator": "5.1.*",
        "symfony/web-link": "5.1.*",
        "symfony/yaml": "5.1.*",
        "twbs/bootstrap": "~4.6",
        "twig/extra-bundle": "^3.3",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "5.1.*",
        "symfony/css-selector": "5.1.*",
        "symfony/debug-bundle": "5.1.*",
        "symfony/maker-bundle": "~1.34",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/stopwatch": "5.1.*",
        "symfony/web-profiler-bundle": "5.1.*",

        "hallerbach-it/symfony-testing": "~1.0",
        "hallerbach-it/testing": "~2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@hallerbach-it.de:~/symfony-testing.git"
        },
        {
            "type": "vcs",
            "url": "git@hallerbach-it.de:~/testing.git"
        }
    ],

    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },

    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },

    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "bin/create-links.sh"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "bin/create-links.sh"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}