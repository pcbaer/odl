{
    "name": "hallerbach-it/odl",
    "description": "A Symfony application that collects, analyzes and visualizes ODL data from BfS.",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Sascha Ternes",
            "email": "info@hallerbach-it.de",
            "role": "Developer"
        }
    ],
    "homepage": "https://www.hallerbach-it.de",
    "time": "2022-03-25",
    "version": "2.1.1",

    "require": {
        "php": "~8.0",
        "ext-ctype": "~8.0",
        "ext-iconv": "~8.0",
        "ext-json": "~8.0",

        "components/jquery": "~3.6",
        "doctrine/annotations": "~1.13",
        "doctrine/doctrine-bundle": "~2.5",
        "doctrine/doctrine-migrations-bundle": "~3.2",
        "doctrine/orm": "~2.11",
        "moment/moment": "~2.29",
        "nnnick/chartjs": "~2.9",
        "phpdocumentor/reflection-docblock": "~5.3",
        "sensio/framework-extra-bundle": "~6.2",
        "symfony/apache-pack": "~1.0",
        "symfony/asset": "~6.0",
        "symfony/console": "~6.0",
        "symfony/dotenv": "~6.0",
        "symfony/expression-language": "~6.0",
        "symfony/flex": "~1.18",
        "symfony/form": "~6.0",
        "symfony/framework-bundle": "~6.0",
        "symfony/http-client": "~6.0",
        "symfony/intl": "~6.0",
        "symfony/mailer": "~6.0",
        "symfony/monolog-bundle": "~3.7",
        "symfony/notifier": "~6.0",
        "symfony/process": "~6.0",
        "symfony/property-access": "~6.0",
        "symfony/property-info": "~6.0",
        "symfony/proxy-manager-bridge": "~6.0",
        "symfony/security-bundle": "~6.0",
        "symfony/serializer": "~6.0",
        "symfony/string": "~6.0",
        "symfony/translation": "~6.0",
        "symfony/twig-bundle": "~6.0",
        "symfony/validator": "~6.0",
        "symfony/web-link": "~6.0",
        "symfony/yaml": "~6.0",
        "twbs/bootstrap": "~4.6",
        "twig/extra-bundle": "~3.3",
        "twig/twig": "~3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5",
        "symfony/browser-kit": "~6.0",
        "symfony/css-selector": "~6.0",
        "symfony/debug-bundle": "~6.0",
        "symfony/maker-bundle": "~1.38",
        "symfony/phpunit-bridge": "~6.0",
        "symfony/stopwatch": "~6.0",
        "symfony/web-profiler-bundle": "~6.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@hallerbach-it.de:~/symfony-testing.git"
        },
        {
            "type": "vcs",
            "url": "git@hallerbach-it.de:~/testing.git"
        }
    ],

    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },

    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true,
            "symfony/flex": true
        }
    },

    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "bin/create-links.sh"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "bin/create-links.sh"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "~6.0"
        }
    }
}
